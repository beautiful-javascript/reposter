{
  "name": "reposter",
  "version": "1.0.0",
  "description": "Easily prepare & schedule your social media reposting campaigns.",
  "main": "index.js",
  "scripts": {
    "build": "babel ./src --out-dir ./lib/",
    "start": "node ./bin/reposter.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beautiful-javascript/reposter.git"
  },
  "keywords": [
    "reposting",
    "social-media",
    "twitter",
    "facebook",
    "buffer",
    "cli"
  ],
  "babel": {
    "plugins": [
      "transform-async-to-generator",
      "transform-object-rest-spread"
    ],
    "presets": [
      "es2015"
    ]
  },
  "author": "Marcin Grzywaczewski (killavus@gmail.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/beautiful-javascript/reposter/issues"
  },
  "preferGlobal": true,
  "homepage": "https://github.com/beautiful-javascript/reposter#readme",
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "bluebird": "^3.4.1",
    "chalk": "^1.1.3",
    "node-uuid": "^1.4.7",
    "progress": "^1.1.8",
    "request": "^2.73.0",
    "striptags": "^2.1.1",
    "xml-stream": "^0.4.5",
    "yargs": "^4.8.0"
  },
  "bin": {
    "reposter-fetch": "./bin/reposter-fetch.js"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0"
  }
}
